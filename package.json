{
  "name": "DSC-Laboratory-Kit",
  "author": "Zhang Haozhi",
  "description": "Software for MIS-DSC Experment System",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently -n=vue,ele -c=green,blue \"npm run dev:vue\" \"npm run dev:ele\"",
    "dev:vue": "vite",
    "dev:ele": "node script/build --env=development --watch",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir --config.asar=false",
    "pretest": "npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue"
  },
  "build": {
    "appId": "com.zhanghaozhi.dsckit"
  },
  "browserslist": [
    "Chrome 102"
  ],
  "devDependencies": {
    "@vitejs/plugin-vue": "1.10.2",
    "cross-env": "7.0.3",
    "concurrently": "^6.4.0",
    "electron": "19.0.8",
    "electron-builder": "23.1.0",
    "electron-devtools-installer": "3.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "eslint": "8.4.1",
    "eslint-plugin-vue": "8.2.0",
    "nano-staged": "0.4.5",
    "playwright": "1.17.1",
    "simple-git-hooks": "2.7.0",
    "vite": "2.7.1"
  },
  "dependencies": {
    "electron-updater": "4.6.5",
    "vue": "3.2.37",
    "vue-router": "4.1.2",
    "echarts": "^5.3.3",
    "element-plus": "2.2.9",
    "@element-plus/icons-vue": "2.0.6",
    "csv-stringify": "^6.0.5"
  }
}
